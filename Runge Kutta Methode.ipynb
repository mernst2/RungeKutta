import numpy as np 
import matplotlib.pyplot as plt

beta=0.4
gamma=0.04
N = 100
#Definitiionen der einzelnen Funktionen

def dSdt(t, S, I):
    return(-beta * S * I / N)

def dIdt(t, S, I):
    return(beta * S * I / N - gamma * I)

def dRdt(t, I):
    return(gamma * I)

